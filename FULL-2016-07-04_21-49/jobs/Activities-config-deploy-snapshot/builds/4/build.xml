    <script>#!groovy

stage &apos;Approve deploy?&apos;
timeout(time: 5, unit: &apos;MINUTES&apos;) {
    input message: &apos;Deploy snapshot?&apos;
}

stage name: &apos;Deploy snapshot&apos;, concurrency: 1
node {
    checkout scm
    
    stash excludes: &apos;build/&apos;, includes: &apos;**&apos;, name: &apos;source&apos;

    sh &apos;./gradlew deployToTest -PrepoId=snapshots -PartifactVersion=LATEST&apos;
}

stage &apos;Health check&apos;
node {
    retry(5) {
        // Give the application some time to start up
        // sleep 30

        unstash &apos;source&apos;
        sh &apos;chmod 755 gradlew&apos;

        sh &apos;SPRING_PROFILES_ACTIVE=testServer1 ./gradlew smokeTest&apos;
    }
}

</script>
